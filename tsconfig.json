{
  "extends": "@tsconfig/strictest",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "jsx": "react",
    "jsxFactory": "_lib.jsx",
    "jsxFragmentFactory": "_lib.jsxs"
  },
  "include": [
    "global.d.ts",
    "global.js",
    "index.ts",
    "jsx-runtime.ts",
    "test.tsx"
  ]
}
